class Bike
{
 int cost;
 String brand;
 String color;
  Bike(int cost,String brand,String color)
	{
	 this.cost=cost;
	 this.brand=brand;
	 this.color=color;
	  return;
	}

	public static void main(String[] args)
	{
		Bike c1=new Bike(5000,"yamaha","pink");
		System.out.println(c1.cost);
		System.out.println(c1.brand);
		System.out.println(c1.color);
	}
}