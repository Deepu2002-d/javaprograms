class For
{

public static void main(String[] args)
{
	for(int i=0;i<=10;i=i+2)
	{
	  System.out.println(i);
	}
	for(int i=1;i<=10;i=i+2)
	{
	  System.out.println(i);
	}
	for(int i=0;i<=10;i=i+2)
	{
	  System.out.println(i*i);
	}
	for(int i=1;i<=10;i=i+2)
	{
	  System.out.println(i*i);
	}
	for(int i=1;i<=10;i=i*2)
	{
	  System.out.println(i);
	}
	for(int i=1;i<=10/2;i=i+2)
	{
	  System.out.println(i);
	}
	for(int i=1;i<=10/2;i=i++)
	{
	  System.out.println(i*2);
	}
	
}
}